<% include ./header %>
    <h1>Authors Page</h1>
    <% authors.forEach((author) => { %>
      <a href="/authors/<%= author['id'] %>"><img src="<%= author['Portrait URL'] %>" alt="<%= author['First Name'] %>" style="height:175px;width:150px;" /></a>
      <a href="/authors/<%= author['id'] %>"><p><%=author['First Name']+' '+author['Last Name']%></p></a>
      <p><%=author['Biography']%></p>
    <% }); %>
  </body>
</html>
